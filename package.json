{
  "name": "fec-capstone",
  "version": "1.0.0",
  "description": "Front end capstone for Hack Reactor",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "nodemon server/server.js & npx webpack serve --open",
    "serve": "npx webpack serve --open",
    "react-dev": "npx webpack --config webpack.config.js --mode development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rpp35-fec-http/fec-capstone.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rpp35-fec-http/fec-capstone/issues"
  },
  "homepage": "https://github.com/rpp35-fec-http/fec-capstone#readme",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "import-lazy": "^4.0.0",
    "nodemon": "^2.0.16",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "msw": "^0.40.0",
    "prettier": "2.6.2",
    "react-test-renderer": "^18.1.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.[jt]sx?$",
    "collectCoverage": true,
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  }
}
